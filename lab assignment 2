//Q.2 The distance between two cities (in kms) is input through the keyboard. Write a program to convert and print this distance in meters, feet, inches and centimeters.
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 2"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	int distance; //Initializing variable
	cout<<"Enter distance in kms: "<<endl;
	cin>>distance; //Assigning the value 
	cout<<"Meters :"<<distance*1000<<" m"<<endl;
	cout<<"Feet: "<<distance*3280<<" ft"<<endl;
	cout<<"Inches: "<<distance*39370<<" inch"<<endl; 
	cout<<"Centimeteres: "<<distance*100000<<" cm"<<endl; //Printing the converted values
	return 0;
}

//Q.3 Enter 4 digit numbers through keyboard. Write a program to obtain the sum of 1st and last digits of this number.
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 3"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	int num,first,second,third,fourth; //Initializing variable
	cout<<"Enter 4 digit number: ";
	cin>>num; //Assigning value to the variable
	first = num/1000; //First time we divide the entered value by 1000 to find the first digit
	num =num%1000; //Modulus function assigns the remainder 3 digit number to the next iteration
	second = num/100; //Dividing the number by 100 gives us the 2nd digit of the number and so on
	num = num%100;
	third = num/10;
	num = num%10;
	fourth = num/1;
	num = num/1;
	cout<<"Sum: "<<first+fourth<<endl; //printing the sum of the digits
	return 0;
}

//Q.4 A0 Paper problem 
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 4"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	int length,breadth,dimension; // intializing variables 
	length = 1189;
	breadth = 841;
	cout<<"A0: 1189 x 841"<<endl;
	for (int i=1;i<=8;++i){  // for loop for every interation of numbers from a0,a1... a8
		dimension=length;
		length=breadth;
		breadth=dimension/2;
		cout<<"A"<<i<<": "<<length<<"mm "<<" x "<<breadth<<"mm "<<endl;	// printing the dimensions of the paper
	}
	return 0;
}



//Q.5 Write the program that gets the total expenses. 
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 5"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	float brand,travel,food,logistics,total; // intializing the variables
	cout<<"Enter branding expenses: "<<endl;
	cin>>brand;  // assigning the values to the variables 
	cout<<"Enter travel expenses: "<<endl;
	cin>>travel;
	cout<<"Enter food expenses: "<<endl;
	cin>>food;
	cout<<"Enter logistics expenses: "<<endl;
	cin>>logistics;
	total = brand+travel+food+logistics; // find the sum of the expenses 
	cout<<"Total expenses: "<<total<<" Rupees"<<endl;  
	cout<<"Branding expenses percentage: "<<brand/total*100<<"%"<<endl;  //finding and printing the percentage of total expenditure
	cout<<"Travel expenses percentage: "<<travel/total*100<<"%"<<endl;
	cout<<"Food expenses percentage: "<<food/total*100<<"%"<<endl;
	cout<<"Logistics expenses percentage: "<<logistics/total*100<<"%"<<endl;
	return 0;
}

//Q.6 cm to m and km
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 6"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	float cm,m,km; //intialising the variables 
	cout<<"Enter distance in cm: "<<endl;
	cin>>cm; 
	m=cm/100;
	km=cm/100000; //converting cm to m and km
	cout<<m<<" meters"<<endl;
	cout<<km<<" kilometers"<<endl;
	return 0;
}


//Q.7 Simple Interest Calculator
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 7"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	float p,r,t,si;  //intialising variables 
	cout<<"Enter principal amount(in rupees): "<<endl;
	cin>>p;
	cout<<"Enter interest rate(%): "<<endl;
	cin>>r;
	cout<<"Enter time period(in years): "<<endl;
	cin>>t;
	si = p*r*t/100;
	cout<<"Simple Interest: "<<si<<" Rupees"<<endl;
	return 0;
}

//Q.8 Marks
#import <iostream>
using namespace std;
int main(){	
	cout<<"Question Number: 8"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	float e,p,c,m,cs,h,total;
	cout<<"Enter english marks out of 100: "<<endl;
	cin>>e;
	cout<<"Enter physics marks out of 100: "<<endl;
	cin>>p;
	cout<<"Enter chemistry marks out of 100: "<<endl;
	cin>>c;
	cout<<"Enter maths marks out of 100: "<<endl;
	cin>>m;
	cout<<"Enter computer science marks out of 100: "<<endl;
	cin>>cs;
	cout<<"Enter hindi marks out of 100: "<<endl;
	cin>>h;
	total = e+p+c+m+cs+h;
	cout<<"Aggregate: "<<total/6<<endl;
	cout<<"Percentage: "<<total/600*100<<"%"<<endl;
	return 0;
}


//Q.9 Lowest no.of notes calculator
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 9"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
    int rs100,rs50,rs10,rs5,rs2,rs1,amt,totaln;
    cout<<"Enter amount: ";
    cin>>amt;

    rs100 = amt/100;
    amt = amt%100;

    rs50 = amt/50;
    amt = amt%50;

    rs10 = amt/10;
    amt = amt%10;

    rs5 = amt/5;
    amt = amt%5;

    rs2 = amt/2;
    amt = amt%2;

    rs1 = amt;  
    if (rs100>0){
        cout<<"100 rupee notes: "<<rs100<<endl;
    } 
    if (rs50>0){
        cout<<"50 rupee notes: "<<rs50<<endl;
    } 
    if (rs10>0){
        cout<<"10 rupee notes: "<<rs10<<endl;
    }    
    if (rs5>0){
        cout<<"5 rupee notes: "<<rs5<<endl;
    }    
    if (rs2>0){
        cout<<"2 rupee notes: "<<rs2<<endl;
    }    
    if (rs1>0){
        cout<<"1 rupee notes: "<<rs1<<endl;
    }
    totaln = rs100 + rs50 + rs10 + rs5 + rs2 + rs1;
    cout<<"Total number of notes: "<<totaln<<endl;
    return 0;
}

//Q.10 Monkey Problem
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 10"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	int x,y,z;
	cout<<"Enter height of the pole (in meters): "<<endl;
	cin>>z;
	cout<<"Enter speed at which the monkey is climbing (in meter per min): "<<endl;
	cin>>x;
	cout<<"Enter distance which the monkey slips (in meters): "<<endl;
	cin>>y;
	
	double time = 0.0;
	int presentheight = 0;
	
	while (presentheight < z){
		time += 1;
		presentheight = presentheight + x;
		if (presentheight >= z){
			break;
		}
	time += 1;
	presentheight = presentheight - y;
	}
	cout<<"The monkey reaches the top in "<<time<<" minutes."<<endl;
}

//Q.11 Leap year conditional operator
#import <iostream>;
using namespace std;
int main(){
	cout<<"Question Number: 11"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
    int year;
    cout<<"Enter the year: "<<endl;
    cin>>year;
    if (year%4==0){
        cout<<"The year "<<year<<" is a leap year!";
    }
    else{
        cout<<"The year "<<year<<" is not a leap year!";
    }
    
    return 0;
}

//Q.12 Swapping numbers using bitwise operator
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 12"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	int n1,n2;
	cout<<"Enter first number: "<<endl;
	cin>>n1;
	cout<<"Enter second number: "<<endl;
	cin>>n2;
	cout<<"Number entered: "<<n1<<n2<<endl;
	n1 = n1^n2;
	n2 = n1^n2;
	n1 = n1^n2;
	cout<<"Swapped number: "<<n1<<n2<<endl;
	return 0;
}


//Q.13 swapping two numbers
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 13"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
    int num1,first,second;
    cout<<"Enter a two digit number: ";
    cin>>num1;
    first = num1/10;
    num1 = num1%10;
    second = num1;
    cout<<second<<first<<endl;
    return 0;
}


