//Q.15 Number of people who attended the event over a course of three days
#import <iostream>
using namespace std;
int main(){
	cout<<"Question Number: 15"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
    int x,n,day1,day2,day3;
    cout<<"Enter total no.of people: ";
    cin>>n;
    x=2*n/7;
    day1 = x;
    day2 = 2*x;
    day3 = x/2;
    cout<<day1<<" people"<<endl;
    cout<<day2<<" people"<<endl;
    cout<<day3<<" people"<<endl;
    return 0;
}


//Q.16 Polynomial solutions from x = 0 to x = 10
#import <iostream>
#import <cmath>
using namespace std;
int main(){
	cout<<"Question Number: 16
"<<endl;
	cout<<"Name : Aaron George"<<endl;
	cout<<"Roll No: 23ECB0F06"<<endl;
	cout<<"______________________________________________________"<<endl;
	cout<<" "<<endl;
	double a= 5.142;
	double b= -0.897;
	double c= 3.145;
	double d= -7.250;
	for (int x=0;x<=10;x++){
		double y = a*pow(x,3) + b*pow(x,2) + c*x + d;
		cout<<"Y("<<x<<")="<<y<<endl;
	} 
	return 0;
}

